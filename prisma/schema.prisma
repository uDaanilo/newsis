// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Veiculo {
  id          Int     @id @default(autoincrement())
  placa       String  @db.VarChar(7)
  rastreado   Boolean
  comprimento Float
  largura     Float
  altura      Float
  cubagem     Float
}
